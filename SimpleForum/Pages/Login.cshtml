@page
@model SimpleForum.Pages.Login
@{
    ViewData["Title"] = "Login";
}

<div class="container">
    <h1>Login</h1>
    <form method="post">
        <input name="email" placeholder="Email" />
        <input name="password" type="password" placeholder="Passowrd" />
        <button type="submit">Login</button>
    </form>
</div>